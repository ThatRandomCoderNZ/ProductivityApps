{
    "manifest_version": 3,

    "name": "Productivity Plugin",
    "description": "This extension will help boost productivity",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "background":{
        "service worker": "background.js"
    },

    "permissions": [
        "activeTab",
        "tabs",
        "tabGroups"
    ]

}